---
---
[
  {% for sticker in site.stickers %}
    {
      "title"    : "{{ sticker.title | escape }}",
      "author"   : "{{ sticker.author }}",
      "tags"     : "{{ sticker.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ sticker.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
